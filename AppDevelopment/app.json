{
  "expo": {
    "name": "MTC",
    "slug": "mtcApp",
    "version": "1.0.0",
    "owner": "lesslax",    
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lesslax.mtcApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
